{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "operator-linebreak": "off",
    "no-plusplus": "off",
    "no-param-reassign": "off",
    "quotes": "off",
    "comma-dangle": "off",
    "class-methods-use-this": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
